<h2>Wines</h2>
<p>Available as of <%= formatted_time(@available_at) %></p>
<%= page_entries_info @wines %>
<ul class="list-unstyled">
<% @wines.each do |wine| %>
  <li class='well'><%= link_to(wine.name, wine) %> a <%= wine.varietal %> from <%= wine.winery %> </li>
<% end %>
</ul>
<%= paginate @wines %>